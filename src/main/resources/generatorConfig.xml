<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

	<!-- 读取资源文件 -->
	<properties resource="config.properties" />

	<classPathEntry
		location="D:\Develop Tool\maven-repository\repository\mysql\mysql-connector-java\5.1.29\mysql-connector-java-5.1.29.jar" />
	<!-- 一个数据库一个context -->
	<context id="LETSBALL" targetRuntime="MyBatis3">
		<!--xml文件是否是覆盖还是合并 -->
		<property name="sqlMapXmlMergeable" value="false" />

		<!-- jdbc连接 -->
		<jdbcConnection driverClass="${driverClassName}"
			connectionURL="${jdbc_url}" userId="${jdbc_username}" password="${jdbc_password}"></jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator targetPackage="com.letsball.entity"
			targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="com.letsball.sqlMaps.mapper"
			targetProject="src/main/resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.letsball.dao" targetProject="src/main/java">
			<property name="enableSubPackages" value="false" />
		</javaClientGenerator>

		<!-- <table schema="letsball" tableName="t_user" domainObjectName="TUser">
			<property name="useActualColumnNames" value="true" />
			<generatedKey column="ID" sqlStatement="DB2" identity="true" />
			<columnOverride column="DATE_FIELD" property="startDate" />
			<ignoreColumn column="FRED" />
			<columnOverride column="LONG_VARCHAR_FIELD" jdbcType="VARCHAR" />
		</table> -->

		<!-- <table tableName="t_dictionary" domainObjectName="TDictionary"></table>
		<table tableName="T_MATCH_FOOTBALL" domainObjectName="TMatchFootball"></table>
		<table tableName="T_TEAM_FOOTBALL" domainObjectName="TTeamFootball"></table>
		<table tableName="t_team_football_member" domainObjectName="TTeamFootballMember"></table>
		<table tableName="t_match_football_member" domainObjectName="TMatchFootballMember"></table> -->
		<table tableName="T_TEAM_FOOTBALL" domainObjectName="TTeamFootball"></table>
		

	</context>
</generatorConfiguration>