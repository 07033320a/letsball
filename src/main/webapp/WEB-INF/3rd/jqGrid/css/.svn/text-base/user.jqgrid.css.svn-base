}
div.ui-jqgrid .ui-icon-triangle-1-s {
background-position: -65px -17px;
}

.inputmouseDownStyle{
		cursor:pointer;
		padding-top:2px;  
		padding-left:2px;
		filter:mask(color=red,offx=3,offy=3,positive=2);
		filter:alpha(opacity=100);
		 -moz-opacity:0.8;
		 -khtml-opacity: 0.8;
		 opacity: 0.8;
}
