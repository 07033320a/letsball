.ui-th-ltr, .ui-jqgrid .ui-jqgrid-htable th.ui-th-ltr {
    border-left: 0 none;
    vertical-align:text-top;
    white-space: normal !important;
}
.ui-jqgrid .ui-jqgrid-htable th div {
    overflow: hidden;
    position: relative;
    height:auto;
}
.ui-btn-down-c {
border: 0;
}
