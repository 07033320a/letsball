.ui-jqgrid .ui-jqgrid-htable th {
	padding: 0;
}
.ui-jqgrid tr.jqgrow td {
	padding: 0;
	height:28px;
}
.ui-jqgrid .ui-jqgrid-bdiv{
	overflow-x: hidden;
}
.ui-jqgrid .loading {
        position: absolute;
        top: 45%;
        left: 45%;
        width: auto;
        z-index: 101;
        padding: 6px;
        margin: 5px;
        text-align: center;
        font-weight: bold;
        display: none;
        visibility: hidden;
        border-width: 2px!important;
}
input.ui-input-text{

} 
.ui-content {
border-width: 0;
overflow: visible;
overflow-x: hidden;
padding: 0;
}
textarea.ui-input-text {
background-image: none;
padding: 0;
line-height: 1.4;
font-size: 16px;
display: block;
width: 97%;
outline: 0;
height:100px;
}

